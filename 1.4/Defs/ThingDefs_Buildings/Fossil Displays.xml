								<!-- poor -->
								<li>(2,0.01)</li>
								<!-- normal -->
								<li>(3,0.01)</li>
								<!-- good -->
								<li>(4,0.01)</li>
								<!-- excellent -->
								<li>(5,0.02)</li>
								<!-- masterwork -->
								<li>(6,0.02)</li>
								<!-- legendary -->
							</points>
						</focusPerQuality>
						<explanationKey>MeditationFocusPerSculpture</explanationKey>
						<explanationKeyAbstract>MeditationFocusPerSculptureAbstract</explanationKeyAbstract>
					</li>
				</offsets>
			</li>
		</comps>
	</ThingDef>
	
