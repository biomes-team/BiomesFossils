								<!-- masterwork -->
								<li>(6,0.28)</li>
								<!-- legendary -->
							</points>
						</curve>
</li>
					<li Class="FocusStrengthOffset_BuildingDefsWithQuality">
						<defs>
							<!-- Dino bone defs -->
							<!-- <li>BMT_FossilSmall_TEST</li> -->
							<!-- <li>BMT_FossilMedium_TEST</li> -->
							<!-- <li>BMT_FossilLarge_TEST</li> --></defs>
						<radius>9.9</radius>
						<maxBuildings>8</maxBuildings>
						<focusPerQuality>
							<points>
								<li>(0,0.0)</li>
								<!-- awful -->
